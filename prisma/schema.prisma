generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Events {
  id              Int      @id @default(autoincrement())
  event_name      String
  event_description String
  event_log       String
  event_type      String
  event_day       String
}

model Team {
  id                Int    @id @default(autoincrement())
  member_name       String
  member_committee  String
  member_designation String
}

model WebAnalytics {
  id        Int      @id @default(autoincrement())
  ip_address String
  date_time  DateTime @default(now())
}

model DownloadLog {
  id         Int      @id @default(autoincrement())
  ip_address String
  date_time  DateTime @default(now())
  image      String
}

model AuditLog {
  id                Int      @id @default(autoincrement())
  user              String
  audit_description String
  date_time         DateTime @default(now())
}

model EventAnalysis {
  id         Int      @id @default(autoincrement())
  event_name String
  ip_address String
  date_time  DateTime @default(now())
}

model Gallery {
  id         Int      @id @default(autoincrement())
  image_path String
  image_name String
  event_name String
  image_type String
}
